.answer-userinfo-content{width:750rpx;height:278rpx;background-color:#4A90E2;margin-bottom: 20rpx;}
.answer-userinfo-box{text-align: center;display: flex;flex-direction: column;justify-content: center;align-items: center;padding: 28rpx 0;}
.answer-userinfo-avatar-box{width:120rpx;height:120rpx;border-radius: 50%;overflow: hidden;}
.answer-userinfo-avatar{width:100%;height:100%;}
.answer-userinfo-nickname{font-size: 32rpx;color: #ffffff;line-height: 60rpx;}
.answer-userinfo-points-box{display: flex;justify-content: center;align-items: center;}
.answer-userinfo-points{font-size: 26rpx;color:#ffffff;}
.answer-points-icon{width: 32rpx;height:32rpx;margin:0 12rpx;}
.my-tap-list-box{background:#ffffff;padding-left: 30rpx;border-top: 1px solid #E5E5E5;}
.my-tab{line-height: 88rpx;padding-left: 60rpx;padding-right: 28rpx; font-size: 34rpx;color: #4A4A4A;}
.my-tab::after{content: " ";display: inline-block;height: 16rpx;width: 16rpx;border-width: 4rpx 4rpx 0 0;border-color: #c7c7cc;border-style: solid; -webkit-transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);float: right;margin-top:38rpx;margin-right: 10rpx;}
.my-mistakes{border-bottom: 1rpx solid #E5E5E5;background: url("data:image/png;base64,") no-repeat 10rpx center;background-size: 31rpx 34rpx;}

.my-exchange{background: url("data:image/png;base64,") no-repeat 10rpx center;background-size: 36rpx 33rpx;}



