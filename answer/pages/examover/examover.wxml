<!--logs.wxml-->
<view class="exam-content">
  <view  class='exam-info'>
    <view class='exam-info-title'>考试结束</view>
    <view class='logoview'><image src='../../images/success.png'> </image></view>
    <view class='exam-info-txt'>答题总数：{{examover.totalNum}}题</view>
    <view class='exam-info-txt'>正确题数：{{examover.rightNum}}题</view>
    <view class='exam-info-txt'>获得积分：{{examover.score}}分</view>
  </view>
  <view  class='exam-btn' bindtap='backhome'>
      <view class="exam-btn-txt">确定</view>
  </view>
</view>
